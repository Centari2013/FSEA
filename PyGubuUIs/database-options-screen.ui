<?xml version='1.0' encoding='utf-8'?>
<interface version="1.2">
  <object class="tk.Toplevel" id="mainwin">
    <property name="background">#ffffff</property>
    <property name="minsize">550|300</property>
    <property name="overrideredirect">False</property>
    <property name="relief">flat</property>
    <property name="takefocus">true</property>
    <property name="title" translatable="yes">F-SEA DATABASE</property>
    <child>
      <object class="tk.Frame" id="button_pane">
        <property name="height">300</property>
        <property name="width">150</property>
        <bind sequence="&lt;1&gt;" handler="callback" add="" />
        <bind sequence="&lt;1&gt;" handler="callback" add="" />
        <layout manager="pack">
          <property name="expand">false</property>
          <property name="fill">both</property>
          <property name="side">left</property>
        </layout>
        <containerlayout manager="grid">
          <property name="anchor">n</property>
          <property name="propagate">False</property>
          <property type="row" id="0" name="minsize">5</property>
          <property type="row" id="all" name="pad">12</property>
        </containerlayout>
        <child>
          <object class="tk.Button" id="empDB_button">
            <property name="text" translatable="yes">Employee Database</property>
            <property name="width">14</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="ipadx">10</property>
              <property name="ipady">10</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="specDB_button">
            <property name="text" translatable="yes">Specimen Database</property>
            <property name="width">14</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="ipadx">10</property>
              <property name="ipady">10</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="deptDB_button">
            <property name="text" translatable="yes">Department Database</property>
            <property name="width">14</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="ipadx">10</property>
              <property name="ipady">10</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="missionDB_button">
            <property name="default">normal</property>
            <property name="text" translatable="yes">Mission Database</property>
            <property name="width">14</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="ipadx">10</property>
              <property name="ipady">10</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="search_pane">
        <property name="background">#ffffff</property>
        <property name="container">false</property>
        <property name="height">300</property>
        <property name="takefocus">true</property>
        <property name="width">350</property>
        <layout manager="pack">
          <property name="expand">false</property>
          <property name="side">right</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
